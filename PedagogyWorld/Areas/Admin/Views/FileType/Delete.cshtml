@model PedagogyWorld.FileType

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>FileType</legend>

    <div class="display-label">FileTypeName</div>
    <div class="display-field">@Model.FileTypeName</div>

    <div class="display-label">FileFileTypes</div>
    <div class="display-field">@(Model.FileFileTypes == null ? "None" : Model.FileFileTypes.Count.ToString())</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}


