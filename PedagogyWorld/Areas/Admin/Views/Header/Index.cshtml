@model IEnumerable<PedagogyWorld.Header>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th></th>
        <th>
            Header1
        </th>
        <th>
            Header2
        </th>
        <th>
            Header3
        </th>
        <th>
            Header4
        </th>
        <th>
            StrandDomain_Id
        </th>
        <th>
            StrandDomain
        </th>
        <th>
            Standards
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
        <td>
			@item.Header1
        </td>
        <td>
			@item.Header2
        </td>
        <td>
			@item.Header3
        </td>
        <td>
			@item.Header4
        </td>
        <td>
			@item.StrandDomain_Id
        </td>
        <td>
			@(item.StrandDomain == null ? "None" : item.StrandDomain.Name)
        </td>
        <td>
			@(item.Standards == null ? "None" : item.Standards.Count.ToString())
        </td>
    </tr>
}

</table>


