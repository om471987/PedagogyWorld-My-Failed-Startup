@model PedagogyWorld.Standard

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>

<h3>Are you sure you want to delete this?</h3>
<fieldset>
    <legend>Standard</legend>

    <div class="display-label">Name</div>
    <div class="display-field">@Model.Name</div>

    <div class="display-label">Name2</div>
    <div class="display-field">@Model.Name2</div>

    <div class="display-label">Name3</div>
    <div class="display-field">@Model.Name3</div>

    <div class="display-label">Name4</div>
    <div class="display-field">@Model.Name4</div>

    <div class="display-label">Header_Id</div>
    <div class="display-field">@Model.Header_Id</div>

    <div class="display-label">Header</div>
    <div class="display-field">@Html.DisplayTextFor(_ => Model.Header).ToString()</div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <input type="submit" value="Delete" /> |
        @Html.ActionLink("Back to List", "Index")
    </p>
}


